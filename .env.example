# Your secret API key
API_KEY=your_api_key_here

# The base URL for the API
BASE_URL=https://openrouter.ai/api/v1

# Controls randomness. Higher values (e.g., 1.0) make the output more diverse and creative.
TEMPERATURE=0.7

# Controls nucleus sampling. A lower value (e.g., 0.9) filters out less probable tokens, making the output more focused.
TOP_P=1.0

# The model to use
MODEL=google/gemini-2.5-flash

# The seed for the model
SEED=42

# The timeout for the model
TIMEOUT=600